{
  "name": "react-template-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:serve": "npm run build && npm run serve",
    "build:treemap": "env-cmd -f .env.treemap npm run build",
    "serve": "serve -s build",
    "lint:es": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "npm run lint:es -- --fix",
    "lint:style:css": "stylelint 'src/**/*.{css,scss,sass}'",
    "lint:style:css:fix": "npm run lint:style:css -- --fix",
    "lint:style:js": "stylelint 'src/**/*.{js,jsx,tsx}'",
    "lint:style:js:fix": "npm run lint:style:js -- --fix",
    "lint:style": "npm run lint:style:css ; npm run lint:style:js",
    "lint:style:fix": "npm run lint:style:css:fix ; npm run lint:style:js:fix",
    "lint:prettier": "prettier --ignore-unknown --check 'src/**/*' ",
    "lint:prettier:fix": "prettier --ignore-unknown --list-different --write 'src/**/*' ",
    "lint:editorconfig": "editorconfig-checker",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.0",
    "reseter.css": "^2.0.0",
    "sass": "^1.43.4",
    "serve": "^12.0.1",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.18",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "craco-alias": "^3.0.1",
    "editorconfig-checker": "^4.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "prettier": "^2.4.1",
    "sass-resources-loader": "^2.2.4",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.6.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
